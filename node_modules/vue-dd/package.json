{
  "name": "vue-dd",
  "version": "1.5.0",
  "description": "Vue.js component for displaying JavaScript objects with circular references, forked and ported to Vue 3",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js",
      "types": "./dist/src/index.d.ts",
      "style": "./dist/style.css"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/src/index.d.ts",
  "style": "./dist/style.css",
  "scripts": {
    "dev": "vite dev --host",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cypress": "cypress run --component",
    "cypress:headed": "cypress run --component --browser chrome --headed --no-exit",
    "build": "cpy ./README.md ../ --cwd=./ && vite build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^5.0.2",
    "@cypress/vue": "^5.0.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.26.2",
    "@vue/test-utils": "^2.2.7",
    "cpy-cli": "^4.2.0",
    "cypress": "^12.3.0",
    "jsdom": "^21.0.0",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-libcss": "^1.0.5",
    "vitest": "^0.27.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/infinite-system/vue-dd"
  },
  "homepage": "https://github.com/infinite-system/vue-dd",
  "bugs": {
    "url": "https://github.com/infinite-system/vue-dd/issues",
    "email": "ekalashnikov@gmail.com"
  },
  "keywords": [
    "Vue",
    "VueJs",
    "vue-dd",
    "dd",
    "object",
    "reactive",
    "tree",
    "view",
    "json",
    "viewer",
    "lazy",
    "circular",
    "inspector"
  ],
  "author": {
    "name": "Evgeny Kalashnikov",
    "email": "ekalashnikov@gmail.com"
  },
  "license": "MIT"
}
