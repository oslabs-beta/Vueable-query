{
  "name": "panel-ts-vue-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite ",
    "build": "npm run type-check && npm run build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build && vite build -c vite.config.extension.ts",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false && tsc --noEmit -p tsconfig.node.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "d3": "^7.8.4",
    "pinia": "^2.0.35",
    "sass": "^1.62.1",
    "vue": "^3.2.47",
    "vue-dd": "^1.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/chrome": "^0.0.236",
    "@types/d3": "^7.4.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.15",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "~5.0.4",
    "vite": "^4.3.4",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  }
}
